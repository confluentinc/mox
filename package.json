{
  "name": "@confluentinc/mox",
  "version": "0.1.3",
  "description": "hybrid proxy and mock server",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/confluentinc/mox.git"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "flow": "flow",
    "lint": "eslint . --report-unused-disable-directives --ext .jsx,.js",
    "jest": "env TZ='America/Los_Angeles' jest",
    "verify": "yarn flow; yarn lint; yarn jest src",
    "clean": "rm -rf lib",
    "copy-flow": "cp src/index.js.flow lib/index.js.flow && cp src/types.js lib/types.js.flow",
    "build": "yarn clean && babel src/ -d lib && yarn copy-flow",
    "package-mox": "yarn verify && yarn build && yarn pack"
  },
  "eslintConfig": {
    "globals": {
      "fixture": false
    },
    "rules": {}
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http-proxy": "^1.18.0",
    "lodash": "^4.17.15",
    "pem": "^1.14.3",
    "request": "^2.88.0",
    "stream-array": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/node": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.7.4",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-flow": "7.7.4",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-flowtype": "4.5.2",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "flow-bin": "0.140.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "webpack-node-externals": "1.7.2"
  }
}
